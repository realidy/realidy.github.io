						<script type="text/javascript">
							$(document).ready(function() {
								$('.news_links a').bind('click', function() {
									if (!$(this).hasClass('sel')) {
										toggle_news($(this).attr('rel'));
										return false;
									}
								});
							});
							function toggle_news(rel) {
								$('.news_links a').removeClass('sel').filter('[rel=' + rel + ']').addClass('sel').closest('div').prependTo('.news_links');
								$('.news_container').hide().filter('.' + rel).show();
							}
