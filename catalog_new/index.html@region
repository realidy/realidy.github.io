<script type="text/javascript">
	var frm2 = $('form[name=SIMPLE_FORM_2]');

	function checkReq2() {
		var isFilled = true;
		var misFld = '';
		frm2.find('.form-required').closest('tr').find('input:visible[type=text],textarea').each(
			function() {
				if (isFilled) {
					var el = $(this);
					if (el.val() == '') {
						misFld = el.attr('name');
						isFilled = false;
					}
				}
			}
		);
		if (!isFilled) {
			alert('Заполните все обязательные поля.');
			setTimeout(
				function() {
					var el = frm2.find('input[name='+misFld+']');
					if (el.size() == 0)
						el = frm2.find('textarea[name='+misFld+']')
					el.focus();
				},
				100
			);
			return false;
		}
		$('input[name=form_text_66]').val('0');
		if ($('.popup_flat .flat_links .notepad_add').size() && !$('.popup_flat').hasClass('popup_hidden'))
			$('input[name=form_text_66]').val($('.popup_content .flat_links .notepad_add').attr('obj_id'));
		return true;
	}

	$(document).ready(
		function() {
			$('input[name=form_text_65]').val('');
			$('#sel_time_from').bind('change', setCallTime);
			$('#sel_time_to').bind('change', setCallTime);
			var ct = $('input[name=form_text_17]').val();
			if (ct == '')
				setCallTime();
			else {
				var t = ct.split(' ');
				$('#sel_time_from').val(t[1]).selectgroup('refresh');
				$('#sel_time_to').val(t[3]).selectgroup('refresh');
			}
			frm2.bind('submit', checkReq2);
		}
	);
	function setCallTime() {
		var f = $('#sel_time_from').val();
		var t = $('#sel_time_to').val();
		var ct = 'с ' + f + ' до ' + t;
		$('input[name=form_text_17]').val(ct);
	}
</script>
</form>					
