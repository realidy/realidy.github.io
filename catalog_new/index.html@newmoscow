<div id="top"></div>
<script>
	var total_pages;
	var preload_pages;
	var scrolltop;
	var moveout;
	var waitajax;
	var options_target;
	var options;
	$(document).ready(function(){
		total_pages = 1;
		preload_pages = 1;
		scrolltop = false;
		moveout = false;
		waitajax = false;
		options_target = '';
		options = {
			url: '',
			type: 'get',
			data: {'ajax':'1'},
			success: function(data) {
				//alert(data);
				$(options_target)[0].innerHTML = "";
				$(options_target)[0].innerHTML = data;
				//$(options_target).html("").html(data);
				if (moveout) {
					moveout = false;
					$(options_target)[0].outerHTML = data;
					//var html = $(options_target).html();
					//$(options_target).parent().append(data);
					//$(options_target).remove();
					//$('.'+options.target).prependTo($('.'+options.target).parent());
				}
				if ($('.total_pages').size() == 1) {
					total_pages = parseInt($('.total_pages').attr('rel'));
				}
				pop_map.init();
				notepad.init();
				init_ajax();
				$('.ajax_realty img.lazy').each(function() {
					if ($(this).parents('.content_slider').size() > 0)
						$(this).lazyload({clss: 'vis'});
					else
						$(this).lazyload();
				});
				content_sl.init();
				$('.ajax_realty .select select').each(function() {
					$(this).selectgroup();
					select_highlight($(this));
 				});
				if (scrolltop) {
					scrolltop = false;
					$("html,body").animate({scrollTop: $('#top').offset().top}, 500);
				}
				waitajax = false;
				setTimeout(function() { $(window).trigger('scroll'); }, 100);
			}
		}
		
		function setOptions($form, $this) {
			options_target = '.'+$form.attr('content');
			options.url = $form.attr('action');
			//options.type = $form.attr('method');
			waitajax = true;
		}
	
		function init_ajax() {
			$('select.ajax_form').each(function(){
				if (!$(this).attr('ajaxed')) {
					$(this).change(function(){
						if (waitajax)
							return false;
						var $form = $(this).parents('form');
						setOptions($form, $(this));
						$form.ajaxSubmit(options);
						preload_pages = 1;
					});
					$(this).attr('ajaxed', true);
				}
			});
			$('form.ajax_form .search_button, form.ajax_form .filter_tip a').each(function(){
				if (!$(this).attr('ajaxed')) {
					$(this).click(function(){
						if (waitajax)
							return false;
						filter.tip_hide();
						var $form = $(this).parents('form');
						setOptions($form, $(this));
						$form.ajaxSubmit(options);
						preload_pages = 1;
						return false;
					});
					$(this).attr('ajaxed', true);
				}
			});
			$('form.ajax_form .clear_filter').each(function(){
				if (!$(this).attr('ajaxed')) {
					$(this).click(function(){
						if (waitajax)
							return false;
						filter.tip_hide();
						filter.clear();
						var $form = $(this).parents('form');
						setOptions($form, $(this));
						$form.ajaxSubmit(options);
						preload_pages = 1;
						return false;
					});
					$(this).attr('ajaxed', true);
				}
			});
			$('.ajax_link').each(function(){
				if (!$(this).attr('ajaxed')) {
					$(this).click(function(){
						if (waitajax)
							return false;
						var $form = $(this).parents('form');
						setOptions($form, $(this));
						$form.ajaxSubmit(options);
						preload_pages = 1;
						return false;
					});
					$(this).attr('ajaxed', true);
				}
			});
			$('.navstring .paging a').each(function(){
				if (!$(this).attr('ajaxed')) {
					$(this).click(function(){
						if (waitajax)
							return false;
						var $form = $(this).parents('form');
						$form.attr('action', $(this).attr('href'));
						setOptions($form, $(this));
						scrolltop = true;
						$form.ajaxSubmit(options);
						return false;
					});
					$(this).attr('ajaxed', true);
				}
			});
			$('.navstring select').each(function(){
				if (!$(this).attr('ajaxed')) {
					$(this).change(function(){
						if (waitajax)
							return false;
						var $form = $(this).parents('form');
						$form.attr('action', $(this).attr('href'));
						setOptions($form, $(this));
						scrolltop = true;
						$form.ajaxSubmit(options);
						return false;
					});
					$(this).attr('ajaxed', true);
				}
			});
		}
		
		init_ajax();
	});
</script>
