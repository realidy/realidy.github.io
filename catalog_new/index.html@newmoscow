<script type="text/javascript">
	var frm1 = $('form[name=SIMPLE_FORM_1]');

	function checkReq1() {
		var isFilled = true;
		var misFld = '';
		frm1.find('.form-required').closest('tr').find('input:visible[type=text],textarea').each(
			function() {
				if (isFilled) {
					var el = $(this);
					if (el.val() == '') {
						misFld = el.attr('name');
						isFilled = false;
					}
				}
			}
		);
		if (!isFilled) {
			alert('Заполните все обязательные поля.');
			setTimeout(
				function() {
					var el = frm1.find('input[name='+misFld+']');
					if (el.size() == 0)
						el = frm1.find('textarea[name='+misFld+']')
					el.focus();
				},
				100
			);
			return false;
		}
		$('input[name=form_text_64]').val('0');
		if ($('.popup_flat .flat_links .notepad_add').size() && !$('.popup_flat').hasClass('popup_hidden'))
			$('input[name=form_text_64]').val($('.popup_content .flat_links .notepad_add').attr('obj_id'));
		
		return true;
	}

	$(document).ready(
		function() {
			$('input[name=form_text_12]').val('');
			$('input[name=form_text_18]').val('');
			$('input[name=form_text_19]').val('http://www.estatet.ru/catalog_new/?newmoscow');
			frm1.bind('submit', checkReq1);
		}
	);
</script>
</form>					
